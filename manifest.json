{
  "manifest_version": 2,
  "name": "rte edit me",
  "description": "Quick link to update sport pages",
  "version":"2",
  "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "content_scripts":[{
        "matches": ["*://*.rte.ie/*"],
        "js":      ["content.js"]
    }],
  "browser_action": {
    "default_icon": "icon_6469.png",
    "default_title":"rte edit me"
  },
  "permissions": ["activeTab"]
}